{
  "name": "blankstring.com",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://blankstring.com",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "bulma": "^0.8.0",
    "fast-xml-parser": "^3.14.0",
    "js-cookie": "^2.2.1",
    "merge": "^1.2.1",
    "react": "^16.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "node-fetch": "^2.6.0",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0"
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "eject": "SKIP_PREFLIGHT_CHECK=true react-scripts eject",
    "predeploy": "npm run build && react-snap && node scripts/create-sitemap.js",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "env": {
      "browser": true,
      "jest": true,
      "jasmine": true,
      "node": true
    },
    "rules": {
      "react/prop-types": 0
    }
  },
  "reactSnap": {
    "include": [
      "/episode",
      "/page",
      "/distribution"
    ]
  }
}
